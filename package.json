{
  "name": "aboutsync",
  "private": true,
  "version": "0.19.0",
  "description": "About Sync Firefox addon",
  "scripts": {
    "build": "webpack --mode=production && web-ext build",
    "dev": "webpack --mode=development --watch",
    "lint": "web-ext lint --ignore-files=data/build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mozilla-extensions/aboutsync.git"
  },
  "author": "Mark Hammond",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/mozilla-extensions/aboutsync/issues"
  },
  "webExt": {
    "build": {
      "overwriteDest": true,
      "filename": "aboutsync.xpi"
    },
    "ignoreFiles": [
      "src"
    ]
  },
  "dependencies": {
    "bootstrap": "^5.1.3",
    "classnames": "^2.2.5",
    "prop-types": "^15.6.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-dom-factories": "^1.0.2",
    "react-inspector": "^2.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-react": "^7.16.7",
    "babel-loader": "^8.2.5",
    "web-ext": "^6.8.0",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  }
}